{
  "name": "tree-finder",
  "version": "0.0.1",
  "description": "browser/tree widget implemented as a custom DOM element. Built on top of regular-table",
  "homepage": "https://github.com/telamonian/regular-tree",
  "bugs": {
    "url": "https://github.com/telamonian/regular-tree/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/telamonian/regular-tree.git"
  },
  "license": "BSD-3-Clause",
  "author": "Max Klein <telamonian@users.noreply.github.com>",
  "workspaces": [
    ".",
    "examples/*"
  ],
  "files": [
    "lib/**/*.{d.ts,js}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "sideEffects": [
    "style/**/*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib/"
  },
  "scripts": {
    "build": "tsc --build",
    "build:webpack": "webpack",
    "clean": "rimraf dist && rimraf lib && rimraf *.tsbuildinfo",
    "example": "webpack-dev-server",
    "example:watch": "webpack-dev-server --watch",
    "watch": "tsc --build --watch",
    "watch:webpack": "webpack --watch"
  },
  "dependencies": {
    "@types/react": "^16.9.52",
    "regular-table": "^0.1.5"
  },
  "devDependencies": {
    "css-loader": "^3.6.0",
    "file-loader": "^6.0.0",
    "rimraf": "^2.5.2",
    "source-map-loader": "^1.0.0",
    "style-loader": "^1.2.1",
    "svg-url-loader": "^6.0.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "webpack": "^5.0.0-beta.17",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
